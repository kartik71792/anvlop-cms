{
  "version": 1,
  "projects": {
    "anvlop-ui": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/anvlop-ui",
      "sourceRoot": "apps/anvlop-ui/src",
      "prefix": "anvlop",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/anvlop-ui",
            "index": "apps/anvlop-ui/src/index.html",
            "main": "apps/anvlop-ui/src/main.ts",
            "polyfills": "apps/anvlop-ui/src/polyfills.ts",
            "tsConfig": "apps/anvlop-ui/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/anvlop-ui/src/favicon.ico",
              "apps/anvlop-ui/src/assets"
            ],
            "styles": ["apps/anvlop-ui/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/anvlop-ui/src/environments/environment.ts",
                  "with": "apps/anvlop-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "anvlop-ui:build",
            "proxyConfig": "apps/anvlop-ui/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "anvlop-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "anvlop-ui:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/anvlop-ui/tsconfig.app.json",
              "apps/anvlop-ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/anvlop-ui/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/anvlop-ui/jest.config.js",
            "tsConfig": "apps/anvlop-ui/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/anvlop-ui/src/test-setup.ts"
          }
        }
      }
    },
    "anvlop-ui-e2e": {
      "root": "apps/anvlop-ui-e2e",
      "sourceRoot": "apps/anvlop-ui-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/anvlop-ui-e2e/cypress.json",
            "tsConfig": "apps/anvlop-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "anvlop-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "anvlop-ui:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/anvlop-ui-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/anvlop-ui-e2e/**"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/api-interfaces/tsconfig.lib.json",
              "libs/api-interfaces/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/api-interfaces/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interfaces/jest.config.js",
            "tsConfig": "libs/api-interfaces/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
